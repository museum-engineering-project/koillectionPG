<span data-value="{{ datum.value|join|default('') }}"></span>
{% set values = datum.value|jsonDecode %}

{% if values|length > 1 %}
    <ul class="datum-list">
        {% for value in values %}
            <li>{{ value|transMlang(app.user.locale) }}</li>
        {% endfor %}
    </ul>
{% else %}
   {{ values|first }}
{% endif %}