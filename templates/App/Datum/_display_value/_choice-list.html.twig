<span data-value="{{ datum.value|join|default('') }}"></span>
{% set values = datum.getOrderedListChoices %}

{% if values|length > 1 %}
    <ul class="datum-list">
        {% for value in values %}
            <li>{{ value|transMlang }}</li>
        {% endfor %}
{% else %}
    {{ values|first|transMlang }}
{% endif %}